{
  "name": "cutedesign",
  "version": "1.1.4",
  "private": true,
  "scripts": {
    "dev": "vite",
    "serve": "vue-cli-service serve --fix --mode local",
    "serve:https": "vue-cli-service serve --fix --mode local --https",
    "build": "vue-cli-service build --mode prod",
    "build-test": "vue-cli-service build --mode test",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --fix",
    "clean:all": "sh ./build/clean-all.sh",
    "compile:all": "sh ./build/build-all.sh",
    "compile:ui": "yarn workspace @cutedesign/ui run build",
    "compile:appendix": "yarn workspace @cutedesign/appendix run build",
    "compile:authenticate": "yarn workspace @cutedesign/authenticate run build",
    "compile:chart": "yarn workspace @cutedesign/chart run build",
    "compile:sort-table": "yarn workspace @cutedesign/sort-table run build",
    "compile:vxe-table": "yarn workspace @cutedesign/vxe-table run build"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "vue-cli-service lint ./src --fix",
      "prettier --write ./src",
      "git add ."
    ]
  },
  "gitHooks": {
    "commit-msg": "node ./scripts/verifyCommit.js"
  },
  "dependencies": {
    "@cutedesign/s3-upload": "^0.1.7",
    "@cutedesign/ct-log": "^1.1.6",
    "@cutedesign/ct-monaco": "^1.1.6",
    "@feta/markdown": "^0.1.2",
    "@feta/markdown-book": "^0.1.0",
    "@vss/video-player": "^0.0.4",
    "@wangeditor/editor": "^5.0.1",
    "@wangeditor/editor-for-vue": "^1.0.1",
    "animejs": "^3.2.1",
    "axios": "^0.26.1",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "^3.21.1",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.28.0",
    "element-ui": "^2.15.6",
    "js-cookie": "^3.0.1",
    "mavon-editor": "^2.10.4",
    "svg-topology": "1.0.21",
    "vue": "2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-codemirror": "^4.0.6",
    "vue-i18n": "8.27.2",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vue-svgicon": "^3.2.9",
    "vuex": "^3.4.0",
    "vuex-module-decorators": "^1.2.0",
    "monaco-editor-webpack-plugin": "4.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.18.6",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@originjs/vite-plugin-require-context": "^1.0.9",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "@vue/cli-plugin-babel": "^4.5.17",
    "@vue/cli-plugin-eslint": "^4.5.17",
    "@vue/cli-plugin-router": "^4.5.17",
    "@vue/cli-plugin-typescript": "^4.5.17",
    "@vue/cli-plugin-vuex": "^4.5.17",
    "@vue/cli-service": "^4.5.17",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-standard": "~5.1.2",
    "@vue/eslint-config-typescript": "~7.0.0",
    "chalk": "^4.1.0",
    "eslint": "~6.8.0",
    "eslint-config-standard": "~14.1.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "~3.4.1",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-vue": "~7.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "path": "^0.12.7",
    "path-to-regexp": "^6.2.0",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.58.3",
    "sass-loader": "^10.1.1",
    "stylelint": "^14.6.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "svg-sprite-loader": "^5.0.0",
    "svgo-loader": "^3.0.1",
    "typescript": "~4.6.3",
    "vite": "^3.1.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue2": "^2.0.2",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "2.6.14",
    "yorkie": "^2.0.0"
  },
  "workspaces": [
    "packages/ui",
    "packages/chart",
    "packages/appendix",
    "packages/sort-table",
    "packages/authenticate",
    "packages/vxe-table"
  ]
}
